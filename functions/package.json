    "lodash": "^4.17.4"
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/core": "^7.0.0-beta.37",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.0",
    "jest": "^22.2.1"
  },
  "main": "dist/index.js"
